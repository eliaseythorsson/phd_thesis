title = {{Effect of Pneumococcal Conjugate Vaccine on Nasopharyngeal Colonization among Immunized and Unimmunized Children in a Community‐Randomized Trial}},
url = {https://academic.oup.com/jid/article-lookup/doi/10.1086/521833},
volume = {196},
