title = {{Re-utilization outcomes and costs of minor acute illness treated at family physician offices, walk-in clinics, and emergency departments.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16926958 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1479579},
volume = {51},
