title = {{Epidemiology of Penicillin Resistant Pneumococci in Iceland}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/9158744 http://www.liebertonline.com/doi/abs/10.1089/mdr.1995.1.121},
