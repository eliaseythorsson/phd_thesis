pmid = {25379659},
publisher = {Public Library of Science},
title = {{All-cause pneumonia hospitalizations in children {\textless}2 years old in sweden, 1998 to 2012: impact of pneumococcal conjugate vaccine introduction.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25379659 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4224441 http://dx.plos.org/10.1371/journal.pone.0112211},
volume = {9},
