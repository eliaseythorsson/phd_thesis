journal = {Otolaryngology - Head and Neck Surgery},
number = {4},
pages = {647--652},
pmid = {15806062},
title = {{Measuring quality of life in preschool children with sore throats and otitis media using the TAPQOL questionnaire}},
volume = {132},
year = {2005}
