issn = {0281-3432},
journal = {Scandinavian Journal of Primary Health Care},
keywords = {antibiotics,antimicrobials,denmark,diag-,general prac-,nostic criteria,tice,unread,upper respiratory tract},
mendeley-tags = {antimicrobials,unread},
