file = {:Users/Elli/Documents/Mendeley Desktop//Anderson et al. - 2017 - Prevalence and determinants of antibiotic exposure in infants A population-derived Australian birth cohort stud.pdf:pdf},
issn = {10344810},
journal = {Journal of Paediatrics and Child Health},
keywords = {antibacterial agents,cephalosporins,drug resistance,infant,infection,prescription},
month = {oct},
