issn = {0264410X},
journal = {Vaccine},
keywords = {Multiple colonisation,Nasopharyngeal colonisation,Pneumococcus,Sample size,Vaccine efficacy,carriage,unread},
mendeley-tags = {carriage,unread},
number = {1},
pages = {159--164},
pmid = {23871614},
publisher = {Elsevier Ltd},
title = {{Design questions for Streptococcus pneumoniae vaccine trials with a colonisation endpoint}},
