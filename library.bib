pmid = {11794168},
title = {{A Comparison of Rate Control and Rhythm Control in Patients with Atrial Fibrillation}},
url = {http://www.nejm.org/doi/abs/10.1056/NEJMoa012295 http://www.ncbi.nlm.nih.gov/pubmed/11794168 http://www.nejm.org/doi/abs/10.1056/NEJMoa021328},
