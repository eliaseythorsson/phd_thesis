pages = {1832--1840},
pmid = {28333294},
title = {{Antibiotic consumption by New Zealand children: exposure is near universal by the age of 5 years}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28333294 https://academic.oup.com/jac/article/3069166/Antibiotic https://academic.oup.com/jac/article-lookup/doi/10.1093/jac/dkx060},
volume = {72},
