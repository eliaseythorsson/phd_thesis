issn = {2164-8344},
journal = {National health statistics reports},
keywords = {Adolescent,Adult,Aged,Child,Diagnostic Tests,Female,Health Care Surveys,Humans,Infant,International Classification of Diseases,Male,Middle Aged,Newborn,Preschool,Routine/utilization,Surgicenters/*utilization,United States,Young Adult},
