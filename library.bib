file = {:Users/Elli/Documents/Mendeley Desktop//Vergison et al. - 2010 - Otitis media and its consequences beyond the earache.pdf:pdf},
isbn = {1474-4457; 1473-3099},
issn = {14733099},
journal = {The Lancet Infectious Diseases},
keywords = {unread},
