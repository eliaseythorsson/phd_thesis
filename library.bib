pages = {268--274},
pmid = {28693043},
title = {{Sensitivity Analysis in Observational Research: Introducing the E-Value.}},
url = {http://annals.org/article.aspx?doi=10.7326/M16-2607 http://www.ncbi.nlm.nih.gov/pubmed/28693043},
volume = {167},
year = {2017}
}
