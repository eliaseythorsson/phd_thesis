issn = {1532-0987},
journal = {The Pediatric infectious disease journal},
keywords = {Child,Cohort Studies,Female,Humans,Immunization Programs,Immunization Programs: statistics {\&} numerical data,Incidence,Infant,Norway,Norway: epidemiology,Pneumococcal Vaccines,Pneumococcal Vaccines: administration {\&} dosage,Pregnancy,Preschool,Questionnaires,Respiratory Tract Infections,Respiratory Tract Infections: epidemiology,Respiratory Tract Infections: prevention {\&} control,unread},
mendeley-tags = {unread},
