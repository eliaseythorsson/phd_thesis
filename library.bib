isbn = {0194-5998},
issn = {0194-5998},
journal = {Otolaryngology -- Head and Neck Surgery},
keywords = {adenotonsillectomy,complications,informed consent,risks,tympanostomy tube insertion},
month = {aug},
number = {2},
pages = {332--339},
pmid = {27048666},
title = {{Informed Consent in Pediatric Otolaryngology: What Risks and Benefits Do Parents Recall?}},
url = {http://search.ebscohost.com/login.aspx?direct=true{\%}7B{\&}{\%}7Ddb=cin20{\%}7B{\&}{\%}7DAN=117149567{\%}7B{\&}{\%}7Dsite=ehost-live http://www.ncbi.nlm.nih.gov/pubmed/27048666 http://oto.sagepub.com/lookup/doi/10.1177/0194599816641910 http://search.ebscohost.com/login.aspx?direct},
