pmid = {21187311},
title = {{National Trends in Visit Rates and Antibiotic Prescribing for Children With Acute Sinusitis}},
url = {http://pediatrics.aappublications.org/cgi/doi/10.1542/peds.2010-1340},
volume = {127},
year = {2011}
}
