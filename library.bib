keywords = {Acute mastoiditis,Acute otitis media,Children,Complications,Guidelines,Incidence},
number = {12},
pages = {1496--1501},
pmid = {21945244},
title = {{Acute mastoiditis in children in Sweden 1993-2007-No increase after new guidelines}},
