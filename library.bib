file = {:Users/Elli/Documents/Mendeley Desktop//Tuerlinckx et al. - 2013 - Evaluation of a WHO-validated serotype-specific serological assay for the diagnosis of pneumococcal etiology.pdf:pdf},
issn = {1532-0987},
journal = {The Pediatric infectious disease journal},
