issn = {1932-6203},
journal = {PloS one},
number = {6},
pages = {e0128606},
pmid = {26039250},
title = {{Otopathogens Detected in Middle Ear Fluid Obtained during Tympanostomy Tube Insertion: Contrasting Purulent and Non-Purulent Effusions.}},
