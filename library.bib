issn = {01492918},
journal = {Clinical Therapeutics},
keywords = {acute otitis media,ciprofloxacin,cost-effectiveness,dexamethasone,ofloxacin,otorrhea,threshold analysis,tympanostomy tubes},
month = {jul},
number = {7},
pages = {1168--1178},
pmid = {15336482},
title = {{A cost threshold analysis of ciprofloxacin-dexamethasone versus ofloxacin for acute otitis media in pediatric patients with tympanostomy tubes}},
