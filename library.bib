pages = {929--37},
pmid = {10491458},
title = {{Streptococcus pneumoniae.}},
url = {http://thorax.bmj.com/content/54/10/929.abstract http://www.ncbi.nlm.nih.gov/pubmed/10491458 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC1745373},
volume = {54},
