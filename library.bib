keywords = {Empirical Bayes,Generalized estimating equations,Generalized linear mixed models,Meta-analysis,unread},
mendeley-tags = {unread},
month = {oct},
number = {24},
pages = {5005--5025},
pmid = {18618416},
publisher = {NIH Public Access},
title = {{Analysis of longitudinal data to evaluate a policy change}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18618416 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3415557},
volume = {27},
