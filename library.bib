file = {:Users/Elli/Documents/Mendeley Desktop//Sj{\"{o}}str{\"{o}}m et al. - 2006 - Clonal and capsular types decide whether pneumococci will act as a primary or opportunistic pathogen.pdf:pdf},
isbn = {1537-6591 (Electronic)$\backslash$r1058-4838 (Linking)},
issn = {1537-6591},
journal = {Clinical infectious diseases : an official publication of the Infectious Diseases Society of America},
keywords = {unread},
mendeley-tags = {unread},
