isbn = {1460-2091 (Electronic)0305-7453 (Linking)},
issn = {14602091},
journal = {Journal of Antimicrobial Chemotherapy},
keywords = {Ambulatory care,Antibiotic consumption,Dose change,Non-linear mixed model,unread},
