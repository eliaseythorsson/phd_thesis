issn = {0095-1137},
journal = {Journal of Clinical Microbiology},
month = {jan},
number = {1},
pages = {63--66},
pmid = {12517826},
publisher = {American Society for Microbiology (ASM)},
title = {{Evaluation of a PCR Assay for Detection of Streptococcus pneumoniae in Respiratory and Nonrespiratory Samples from Adults with Community-Acquired Pneumonia}},
