journal = {Infection and Immunity},
keywords = {invasiveness,unread},
mendeley-tags = {invasiveness,unread},
number = {1},
pages = {431--435},
pmid = {15618181},
title = {{Invasiveness of serotypes and clones of Streptococcus pneumoniae among children in Finland}},
volume = {73},
