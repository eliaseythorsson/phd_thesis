doi = {10.1097/INF.0000000000001810},
file = {:Users/Elli/Documents/Mendeley Desktop//Palmu et al. - 2017 - Impact of National Ten-Valent Pneumococcal Conjugate Vaccine Programme on Reducing Antimicrobial Use and Tympanost.pdf:pdf},
